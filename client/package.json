{
  "name": "Circonference",
  "version": "0.0.1",
  "description": "A thing that manages MH conferences",
  "author": "Sam Bleckley <sam@mutuallyhuman.com>",
  "homepage": "http://www.mutuallyhuman.com",
  "license": "UNLICENSED",
  "repository": "'sacomin",
  "scripts": {
    "build-dev": "webpack --display-error-details --config build_scripts/webpack.config.dev.js",
    "build": "webpack --display-error-details --config build_scripts/webpack.config.prod.js",
    "generate": "plop --plopfile build_scripts/plopfile.js",
    "start": "node build_scripts/server.js",
    "test": "jest",
    "deploy": "./build_scripts/deploy.sh"
  },
  "dependencies": {
    "brim": "^1.0.10",
    "classnames": "^2.2.5",
    "lodash": "^4.17.2",
    "mobx": "^2.7.0",
    "mobx-react": "^4.0.4",
    "moment": "^2.17.1",
    "platform": "^1.3.3",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-addons-transition-group": "^15.4.1",
    "react-datepicker": "^0.39.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^3.0.0",
    "scream": "^2.0.7",
    "screenfull": "^3.0.2",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "@types/classnames": "0.0.32",
    "@types/enzyme": "^2.5.39",
    "@types/immutability-helper": "^2.0.15",
    "@types/jasmine": "^2.5.38",
    "@types/lodash": "^4.14.41",
    "@types/moment": "^2.13.0",
    "@types/react": "^0.14.52",
    "@types/react-addons-test-utils": "^0.14.15",
    "@types/react-addons-transition-group": "^0.14.17",
    "@types/react-dnd": "^2.0.31",
    "@types/react-dnd-html5-backend": "^2.1.6",
    "@types/react-dom": "^0.14.19",
    "@types/react-hot-loader": "^3.0.0",
    "@types/react-router": "^2.0.41",
    "@types/requirejs": "^2.1.28",
    "@types/whatwg-fetch": "0.0.32",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "browser-sync": "^2.18.2",
    "browser-sync-webpack-plugin": "^1.1.3",
    "css-loader": "^0.25.0",
    "css-to-string-loader": "^0.1.2",
    "enzyme": "^2.6.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "inquirer-directory": "^2.1.0",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "jest-cli": "^17.0.3",
    "jest-enzyme": "^2.0.0",
    "node-sass": "^3.13.0",
    "plop": "^1.7.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.0.2",
    "source-map-support": "^0.4.6",
    "style-loader": "^0.13.1",
    "typescript": "^2.1.4",
    "typings-for-css-modules-loader": "^1.0.0",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/build_scripts/jest/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      ".*": "<rootDir>/build_scripts/jest/preprocessor.js"
    },
    "testRegex": "app/.*\\.spec\\.(ts|tsx)$"
  }
}
