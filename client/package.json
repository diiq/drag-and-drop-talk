{
  "name": "pigmentor",
  "version": "0.0.1",
  "description": "Yet Another Color Picking App",
  "author": "Sam Bleckley <sam@climatum.com>",
  "homepage": "https://github.com/diiq/whatever",
  "license": "MIT",
  "repository": "https://",
  "scripts": {
    "build-dev": "webpack --display-error-details --config build_scripts/webpack.config.dev.js",
    "build": "webpack --display-error-details --config build_scripts/webpack.config.prod.js",
    "generate": "plop --plopfile build_scripts/plopfile.js",
    "start": "webpack-dev-server --config build_scripts/webpack.config.dev.js --progress --colors",
    "test": "jest",
    "deploy": "./build_scripts/deploy.sh",
    "size": "webpack --config build_scripts/webpack.config.prod.js --json"
  },
  "dependencies": {
    "actioncable": "^5.1.1",
    "chroma-js": "^1.3.6",
    "dashify": "^0.2.2",
    "deep-equal": "^1.0.1",
    "download.js": "^1.0.0",
    "downloadjs": "^1.4.7",
    "es6-promise-polyfill": "^1.2.0",
    "font-awesome": "^4.7.0",
    "glamor": "^2.20.24",
    "he": "^1.1.1",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "jszip": "^3.1.5",
    "lodash.throttle": "^4.0.8",
    "mobx": "^3.1.10",
    "mobx-react": "^4.2.1",
    "moment": "^2.18.1",
    "nosleep.js": "^0.7.0",
    "prop-types": "^15.5.8",
    "react": "^16",
    "react-dom": "^16",
    "react-flip-move": "^2.9.14",
    "react-helmet": "^5.2.0",
    "react-overlays": "^0.7.4",
    "react-select": "^1.0.0-rc.10",
    "react-transition-group": "2.3.0-beta.0",
    "scrollparent": "^2.0.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/actioncable": "^0.0.2",
    "@types/chroma-js": "^1.3.4",
    "@types/downloadjs": "^1.4.0",
    "@types/enzyme": "^2.7.9",
    "@types/he": "^0.5.29",
    "@types/history": "^4.5.0",
    "@types/jest": "^19.2.2",
    "@types/lodash.throttle": "^4.1.3",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^16.0.5",
    "@types/react-addons-test-utils": "^0.14.15",
    "@types/react-dom": "^16",
    "@types/react-flip-move": "^2.9.0",
    "@types/react-helmet": "^5.0.3",
    "@types/react-hot-loader": "^3.0.0",
    "@types/react-overlays": "^0.6.4",
    "@types/react-select": "^1.0.59",
    "@types/react-transition-group": "^2.0.4",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "css-loader": "^0.25.0",
    "css-to-string-loader": "^0.1.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "inquirer-directory": "^2.1.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "plop": "^1.7.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "source-map-support": "^0.4.6",
    "style-loader": "^0.13.1",
    "ts-jest": "^20.0.5",
    "typescript": "^2.6.1",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-monitor": "^1.0.13"
  },
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 3"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/app"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg|css|mp4)$": "<rootDir>/build_scripts/jest/fileMock.js",
      "\\./history$": "<rootDir>/app/router/__mocks__/history.ts",
      "\\./(.*)-service$": "<rootDir>/app/models/$1/__mocks__/$1-service.ts"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/build_scripts/jest/test-setup.js",
    "transform": {
      ".(ts|tsx)": "<rootDir>/build_scripts/jest/preprocessor.js"
    },
    "testRegex": "app/.*\\.spec\\.(ts|tsx)$",
    "globals": {
      "PRODUCTION": false
    }
  }
}
